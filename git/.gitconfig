[core]
  editor = vim
[color]
  branch = auto
  interactive = auto
  ui = true
  pager = true
[color "diff-highlight"]
  oldNormal = red bold
  oldHighlight = red bold 52
  newNormal = green bold
  newHighlight = green bold 22
[color "status"]
  added = green
  changed = blue
  untracked = red
[color "diff"]
  meta = yellow
  frag = magenta bold
  commit = yellow bold
  old = red bold
  new = green bold
  whitespace = red reverse
[push]
  default = current
